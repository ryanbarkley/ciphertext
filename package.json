{
  "name": "cryptogram",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/ryanbarkley/cryptogram"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint **/*.{js,jsx,ts,tsx}",
    "check:style": "prettier --check .",
    "fix:style": "prettier --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/faker": "^5.1.5",
    "@types/jest-axe": "^3.5.1",
    "faker": "^5.1.0",
    "jest-axe": "^4.1.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 45,
        "branches": 8,
        "functions": 55,
        "lines": 45
      },
      "./src/ciphers/**.{ts,tsx}": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    }
  }
}
