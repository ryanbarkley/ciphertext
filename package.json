{
  "name": "cryptogram",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/ryanbarkley/cryptogram"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "jest-junit": "^13.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --run-in-band --coverage --reporters=\"jest-junit\" --watchAll=false",
    "test:e2e": "cross-env BROWSER=none start-server-and-test start http://localhost:3000 cypress:run",
    "test:lighthouse": "yarn build && lhci autorun",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "eject": "react-scripts eject",
    "check:types": "yarn tsc --noEmit",
    "check:lint": "eslint **/*.{js,jsx,ts,tsx} --ignore-path ./.gitignore",
    "check:style": "prettier --ignore-path ./.gitignore --check .",
    "fix:style": "prettier --ignore-path ./.gitignore --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lhci/cli": "^0.6.1",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/react-hooks": "^5.0.0",
    "@testing-library/user-event": "^12.6.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "faker": "^5.1.0",
    "jest-axe": "^4.1.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.7",
    "web-vitals": "^1.0.1",
    "workbox-background-sync": "^6.0.2",
    "workbox-broadcast-update": "^6.0.2",
    "workbox-cacheable-response": "^6.0.2",
    "workbox-core": "^6.0.2",
    "workbox-expiration": "^6.0.2",
    "workbox-google-analytics": "^6.0.2",
    "workbox-navigation-preload": "^6.0.2",
    "workbox-precaching": "^6.0.2",
    "workbox-range-requests": "^6.0.2",
    "workbox-routing": "^6.0.2",
    "workbox-strategies": "^6.0.2",
    "workbox-streams": "^6.0.2"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 60,
        "functions": 100,
        "lines": 100
      },
      "./src/ciphers/**.{ts,tsx}": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./src/Algorithms.ts": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./src/{index,reportWebVitals,service-worker,serviceWorkerRegistration}.{ts,tsx}": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}
