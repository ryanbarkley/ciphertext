{
  "name": "cryptogram",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "url": "https://github.com/ryanbarkley/cryptogram"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "yarn test --coverage --watchAll=false && yarn test:e2e",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "cypress:open": "yarn cypress open",
    "cypress:run": "yarn cypress run",
    "eject": "react-scripts eject",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint **/*.{js,jsx,ts,tsx}",
    "check:style": "prettier --ignore-path ./gitignore --check .",
    "fix:style": "prettier --ignore-path ./gitignore --write ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/jest-axe": "^3.5.1",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "cypress": "^6.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "faker": "^5.1.0",
    "jest-axe": "^4.1.0",
    "prettier": "^2.2.1",
    "start-server-and-test": "^1.11.7"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 60,
        "functions": 100,
        "lines": 100
      },
      "./src/ciphers/**.{ts,tsx}": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./src/Algorithms.ts": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      },
      "./src/index.ts": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      },
      "./src/serviceWorker.ts": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  }
}
