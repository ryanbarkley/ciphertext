{
  "ci": {
    "collect": {
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "apple-touch-icon": "off",
        "maskable-icon": "off",
        "splash-screen": "off",
        "installable-manifest": "off",
        "unused-javascript": "off",
        "legacy-javascript": "off",
        "max-potential-fid": "off",
        "render-blocking-resources": "off",
        "csp-xss": "off"
      }
    }
  }
}
